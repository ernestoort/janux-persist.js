{
    "appenders": [
        {
            "type": "file",
            "filename": "test/test.log",
            "layout": {
                "type": "pattern",
                "pattern": "%d | %p | %c | %m"
            }
        },
        {
            "type": "console",
            "layout": {
                "type": "pattern",
                "pattern": "%[%p | %c |%] %m"
            }
        }
    ],
    "replaceConsole": "false",
    "levels": {
        "console": "ERROR",
        "test": "WARN",
        "PermissionContext_test": "WARN",
        "PermissionHolder_test": "WARN",
        "Role_test": "WARN",
        "UserServiceMock": "WARN"
    }
}
